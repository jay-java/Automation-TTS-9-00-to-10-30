package core;

//byte,short,int,long,float,double,boolean,char
//array,string,class
public class P002_Datatypes {
	public static void main(String[] args) {
		byte b = 12;// 1
		System.out.println(b);
		short s = 12;// 2
		System.out.println(s);
		int i = 12;// 4
		System.out.println(i);
		long l = 12;// 8
		float f = 3.14f;//4
		double d = 3.14;//8
		boolean bool = false;
		char c = 'q';
	}
}
