package core;

//1.assignment => =,+=,-=,*=,/=
//2.arithmetic => +,-,*,/,%
//3.logical => &&(and),||(or),!(not)
//4.conditional => >,<,>=,<=,!=
//5.unary => ++,--
public class P003_Operators {
	public static void main(String[] args) {
		int i = 1, j = 2, k;
		k = i+j;
		System.out.println("addition of "+i+" and "+j+" = "+k);
		System.out.println("addition of i and j = "+(i+j));
		k = i-j;
		System.out.println(k);
	}
	
}
