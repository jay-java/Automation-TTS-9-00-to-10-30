package core;

import java.util.HashMap;
import java.util.Map;

public class P025_Map {
	public static void main(String[] args) {
		Map map = new HashMap();
		map.put(1, "c");
		map.put(2, "c++");
		map.put(3, "java");
		map.put("python", 4);
		map.put(1.34, "ruby");
		map.put(45, false);
		
	}
	
}
