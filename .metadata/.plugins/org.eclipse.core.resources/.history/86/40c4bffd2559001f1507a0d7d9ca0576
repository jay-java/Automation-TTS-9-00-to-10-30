package core;
class Data{
	Data(){
		System.out.println("default cons");
	}
	Data(int i){
		System.out.println("second cons");
	}
	public void call() {
		System.out.println("call fun in data class");
	}
}
public class P012_Constructor {
	public static void main(String[] args) {
		Data d = new Data();
		d.call();
	}
}
