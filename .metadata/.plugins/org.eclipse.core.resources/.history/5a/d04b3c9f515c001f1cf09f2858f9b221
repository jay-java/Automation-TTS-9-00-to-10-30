package core;
//1.By Thread class
//2.By Runnable interface

public class P021_Thread {
	public static void main(String[] args) {
		Thread t = new Thread();
		System.out.println(t.currentThread());
		t.setName("MyThread");
		System.out.println(t);
		for(int i=1;i<=5;i++) {
			System.out.println(i+" : "+t);
		}
	}
}
