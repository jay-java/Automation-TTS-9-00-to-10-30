package core;
class Overload{
	public int add(int i,int j) {
		return i+j;
	}
	public int add(int i,int j,int k) {
		return i+j+k;
	}
}
public class P016_Polymorphism {
	public static void main(String[] args) {
		Overload o = new Overload();
		int res1 = o.add(1, 2);
		int res2 = o.add(1, 2, 3);
	}
}
